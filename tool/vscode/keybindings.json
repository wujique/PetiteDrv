// 将键绑定放在此文件中以覆盖默认值auto[]
[
    {
        "key": "ctrl+f8",
        "command": "editor.action.marker.nextInFiles",
        "when": "editorFocus"
    },
    {
        "key": "f8",
        "command": "-editor.action.marker.nextInFiles",
        "when": "editorFocus"
    },
    {
        "key": "f8",
        "command": "highlightwords.addHighlight"
    }
]